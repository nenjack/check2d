{
  "name": "check2d",
  "version": "9.36.0",
  "description": "detect collisions 2d between all possible kinds of shapes. It uses Bounding Volume Hierarchy (BVH) for sweep wide quick tree trim phase, and the Separating Axis Theorem (SAT) for accurate detection inside nearby groups. This library supports RayCasting, offsets, rotation, scaling, and optimizations. Also includes flags for groups filtering - making it an ideal choice for real-time online gaming and simulations.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "start": "chef-express dist/demo --debug",
    "restart": "yarn build && yarn start",
    "test": "jest --silent --verbose --forceExit",
    "dev": "webpack serve --port 3000",
    "amend": "yarn precommit && git commit -a --am --no-edit",
    "use-latest-quickselect-macos": "sed -i '' -e \"s_'quickselect'_'./quickselect'_g\" src/external/rbush.js",
    "use-latest-quickselect-linux": "sed -i -r \"s_'quickselect'_'./quickselect'_g\" src/external/rbush.js",
    "use-latest-quickselect": "yarn use-latest-quickselect-macos || yarn use-latest-quickselect-linux",
    "copy-rbush": "cp node_modules/rbush/index.js src/external/rbush.js",
    "copy-quickselect": "cp node_modules/quickselect/index.js src/external/quickselect.js",
    "copy-npm-modules": "yarn copy-rbush && yarn copy-quickselect && yarn use-latest-quickselect",
    "prebuild": "yarn copy-npm-modules",
    "build": "yarn build:commonjs && yarn build:esm",
    "postbuild": "yarn test && yarn build:demo",
    "prebuild:commonjs": "rimraf dist",
    "build:commonjs": "tsc",
    "prebuild:esm": "rimraf esm",
    "build:esm": "rollup -c",
    "build:docs": "typedoc --customCss typedoc.css",
    "postbuild:docs": "yarn docs-from-main && cp -r dist/demo docs",
    "build:demo": "webpack",
    "prebuild:docs": "rimraf docs",
    "docs": "yarn build:docs && chef-express docs --debug --port 3000",
    "docs-from-main-macos": "find docs -type f -name '*.html' -exec sed -i '' -e 's_/blob/[^/]+/_/blob/main/_g' {} +",
    "docs-from-main-linux": "find docs -type f -name '*.html' -exec sed -i -r 's_/blob/[^/]+/_/blob/main/_g' {} +",
    "docs-from-main": "yarn docs-from-main-macos || yarn docs-from-main-linux",
    "demo": "yarn build:demo && npm start",
    "precommit": "yarn lint && yarn build && yarn format && yarn build:docs",
    "lint": "eslint src --fix",
    "format": "prettier . --write",
    "benchmark": "./clean-dist.sh && yarn benchmark-insertion && yarn benchmark-stress",
    "benchmark-stress": "node -e 'import(\"./dist/benchmarks/index.js\").then(({ stressBenchmark }) => stressBenchmark());'",
    "benchmark-insertion": "node -e 'import(\"./dist/benchmarks/index.js\").then(({ insertionBenchmark }) => insertionBenchmark());'",
    "update": "npx npm-check-updates -u && yarn add tinybench@^5 -D"
  },
  "repository": "git://github.com/nenjack/check2d.git",
  "files": [
    "esm/index.js",
    "dist/*.*",
    "dist/bodies",
    "dist/external"
  ],
  "keywords": [
    "javascript",
    "typescript",
    "library",
    "computational-geometry",
    "alghoritm",
    "computational-geometry-alghoritm",
    "collisions",
    "collision-checking",
    "concave",
    "rotation",
    "angle",
    "separating-axis-theorem",
    "bounding-volume-hierarchy",
    "Circle",
    "Polygon",
    "Box",
    "Ellipse",
    "Line",
    "Point"
  ],
  "author": "Jacek Pietal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nenjack/check2d/issues"
  },
  "homepage": "https://nenjack.github.io/check2d/",
  "dependencies": {
    "@types/sat": "^0.0.35",
    "poly-decomp-es": "^0.4.2",
    "sat": "^0.9.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/node": "^25.0.3",
    "@types/rbush": "^4.0.0",
    "chef-express": "^3.2.3",
    "eslint": "~9.39.2",
    "eslint-plugin-json": "^4.0.1",
    "html-webpack-plugin": "^5.6.5",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "quickselect": "^3.0.0",
    "random-seed": "^0.3.0",
    "rbush": "^4.0.1",
    "rimraf": "^6.1.2",
    "rollup": "^4.53.5",
    "tinybench": "^5",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "typedoc": "^0.28.15",
    "typescript": "^5",
    "webpack": "^5.104.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "resolutions": {
    "@types/node": "^22.4.1",
    "minimist": "^1.2.6",
    "terser": "^5.14.2",
    "json5": "^2.2.2",
    "semver": "^7.5.3",
    "escodegen": "^2.1.0",
    "express": "^4.19.2"
  }
}
